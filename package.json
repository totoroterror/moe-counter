{
  "name": "moe-counter",
  "description": "Moe Counter rewritten in TypeScript",
  "version": "1.0.0",
  "private": true,
  "author": "totoroterror",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development node --nolazy -r ts-node/register src/index.ts",
